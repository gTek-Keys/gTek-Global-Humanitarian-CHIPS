{
  "name": "gtek-humanitarian-club",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "vercel-build": "next build",
    "vercel-dev": "next dev",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "env:pull": "vercel env pull .env.local",
    "env:add": "vercel env add",
    "setup:gcp": "bash ./scripts/setup-gcp-infrastructure.sh",
    "setup:supabase": "bash ./scripts/setup-supabase.sh",
    "docker:build": "docker build -t gtek-humanitarian-chips .",
    "docker:run": "docker run -p 3000:3000 gtek-humanitarian-chips",
    "docker:push": "./scripts/docker-login.sh && docker push ceptokrem/gtek-humanitarian-chips:latest",
    "ghcr:login": "./scripts/ghcr-login.sh",
    "ghcr:push": "./scripts/ghcr-login.sh && docker push ghcr.io/gtek-keys/gtek-humanitarian-chips:latest"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-table": "^8.20.5",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.16",
    "gridjs": "^6.2.0",
    "gridjs-react": "^6.1.1",
    "heroicons": "^2.2.0",
    "lucide-react": "^0.446.0",
    "mongodb": "^6.8.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swr": "^2.2.5",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.3",
    "husky": "^9.1.7",
    "jest-environment-jsdom": "^30.1.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
