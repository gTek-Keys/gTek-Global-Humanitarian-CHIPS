{
  "licenseKey": "6320711a-30c6-4260-b770-e4cb6272c3a9",
  "premium": {
    "enabled": true,
    "features": {
      "timeline": true,
      "workspaces": true,
      "focus": true,
      "deepLinks": true,
      "launchpad": true
    }
  },
  "plusFeatures": {
    "enabled": true
  },
  "advanced": {
    "repositorySearchDepth": 3,
    "fileHistoryFollowsRenames": true,
    "similarityThreshold": 50
  },
  "blame": {
    "highlight": {
      "enabled": true,
      "locations": ["gutter", "line", "overview"]
    },
    "avatars": true,
    "compact": false,
    "dateFormat": "MMMM Do, YYYY h:mma",
    "format": "${author|10} ${agoOrDate|14-} ${message}",
    "heatmap": {
      "enabled": true,
      "location": "right"
    }
  },
  "codeLens": {
    "enabled": true,
    "authors": {
      "enabled": true,
      "command": "gitlens.toggleFileBlame"
    },
    "recentChange": {
      "enabled": true,
      "command": "gitlens.showQuickCommitDetails"
    },
    "scopes": ["document"]
  },
  "currentLine": {
    "enabled": true,
    "pullRequests": {
      "enabled": true
    }
  },
  "hovers": {
    "enabled": true,
    "annotations": {
      "changes": true,
      "details": true,
      "over": "line"
    },
    "currentLine": {
      "enabled": true,
      "over": "line"
    },
    "avatars": true,
    "pullRequests": {
      "enabled": true
    }
  },
  "views": {
    "branches": {
      "showBranchComparison": "working"
    },
    "commits": {
      "showBranchComparison": "working"
    },
    "fileHistory": {
      "enabled": true,
      "avatars": true
    },
    "lineHistory": {
      "enabled": true,
      "avatars": true
    },
    "repositories": {
      "enabled": true,
      "autoRefresh": true
    },
    "timeline": {
      "enabled": true,
      "avatars": true
    }
  },
  "graph": {
    "layout": "editor",
    "minimap": {
      "enabled": true
    },
    "pullRequests": {
      "enabled": true
    },
    "scrollRowPadding": 2,
    "showGhostRefsOnRowHover": true,
    "showRemoteNames": true
  },
  "integrations": {
    "enabled": true,
    "github": {
      "enabled": true,
      "pullRequests": {
        "enabled": true
      },
      "issues": {
        "enabled": true
      }
    }
  },
  "remotes": [
    {
      "type": "GitHub",
      "domain": "github.com",
      "name": "GitHub"
    }
  ],
  "defaultGravatarsStyle": "identicon",
  "outputLevel": "errors",
  "showWelcomeOnInstall": false,
  "showWhatsNewAfterUpgrades": false
}
